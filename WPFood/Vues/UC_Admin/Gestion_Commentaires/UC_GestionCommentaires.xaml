<UserControl x:Class="WPFood.Vues.UC_Admin.UC_GestionCommentaires"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFood.Vues.UC_Admin"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Loaded="Grid_Loaded">
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Button Content="Retour au menu admin" Grid.Row="0" Height="50" Width="150" VerticalAlignment="Top" HorizontalAlignment="Left" Click="RetourMenuAdmin_Click"/>
        <TextBlock Text="Gestion des commentaires" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25" TextDecorations="Underline" FontWeight="Bold"/>

        <!-- /////////////////////////// -->

        <Grid Grid.Row="1">

        </Grid>
    </Grid>
</UserControl>
